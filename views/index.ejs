<!DOCTYPE html>
<html lang="fr">
<head>
 	<meta charset="UTF-8">
 	<title>Mon Application</title>
 	<link rel="stylesheet" type="text/css" href="css/style.css">
 	<link rel="stylesheet" type="text/css" href="css/normalize.css">
</head>
<body>
 <h3>Liste des membres</h3>

 <table class="tableau">
 	<% let noLigne = 0 %>
		<% for(elm of adresses) {%>
		<% if(noLigne==0) { %>
		<tr>
			<% noLigne++ %>
			<% for(p in elm) {%>
			<td><%= p %></td>
			<% } %>
			<td><a href="/formulaire">Ajouter</a></td>
		</tr>
		<% } %>
		<tr>
			<% for(p in elm) {%>
			<td><%= elm[p] %></td>
			<% } %>
			<td><a href="/detruire/<%= elm['_id'] %>">DÃ©truire</a></td>
		</tr>
	<% } %>
 	</table>
</body>
</html>